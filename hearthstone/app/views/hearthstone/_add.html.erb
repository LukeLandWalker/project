<p>Already have <%= @number %> cards (less than 30)</p>
<% i = 0 %>
<table>
  <tr>
    <th>name</th>
    <th>type</th>
    <th>cost</th>
    <th>number</th>
  </tr>
<% while (i < @cardArray.size) %>
  <tr>
    <th><%= @cardArray[i].name %></th>
    <th><%= @cardArray[i].types %></th>
    <th><%= @cardArray[i].cost %></th>
    <th><%= @TempCards[i].number %></th>
  </tr>
<% i += 1 %>
<% end %>
</table>
